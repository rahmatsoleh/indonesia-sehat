{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style" : 0,
        "no-undef" : 0,
        "eol-last": 0,
        "indent": 0,
        "padded-blocks": 0,
        "no-trailing-spaces": 0,
        "quotes": 0,
        "semi": 0,
        "arrow-parens": 0,
        "no-use-before-define": 0,
        "no-restricted-syntax":0,
        "prefer-const": 0,
        "no-plusplus": 0,
        "space-before-blocks": 0,
        "eqeqeq": 0,
        "no-alert": 0,
        "no-console": 0,
        "object-curly-newline": 0
    }
}
